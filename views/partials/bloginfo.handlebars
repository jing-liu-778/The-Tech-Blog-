<div class="text-center">
  <h2>{{title}}</h2>
  <p>{{{get_emoji}}}</p>
  <p>{{blog_text}}</p>
  <strong>Posted by {{user.name}} on {{format_date date_created}}</strong>
  <div class="col-md-4">
    <button class="btn btn-sm btn-danger" data-id="{{blog.id}}">DELETE</button>
  </div>
  <div class="col-md-4">
    <button class="btn btn-sm btn-success" data-id="{{blog.id}}">UPDATE</button>
  </div>
</div>
